{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "formulaire-demandeur-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/formulaireDemandeur/FormulaireDemandeurWebPart.js",
          "manifest": "./src/webparts/formulaireDemandeur/FormulaireDemandeurWebPart.manifest.json"
        }
      ]
    },
    "dashboard-demandeur-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardDemandeur/DashboardDemandeurWebPart.js",
          "manifest": "./src/webparts/dashboardDemandeur/DashboardDemandeurWebPart.manifest.json"
        }
      ]
    },
    "demandeur-dashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/demandeurDashboard/DemandeurDashboardWebPart.js",
          "manifest": "./src/webparts/demandeurDashboard/DemandeurDashboardWebPart.manifest.json"
        }
      ]
    },
    "approbateur-dashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/approbateurDashboard/ApprobateurDashboardWebPart.js",
          "manifest": "./src/webparts/approbateurDashboard/ApprobateurDashboardWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FormulaireDemandeurWebPartStrings": "lib/webparts/formulaireDemandeur/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "DashboardDemandeurWebPartStrings": "lib/webparts/dashboardDemandeur/loc/{locale}.js",
    "DemandeurDashboardWebPartStrings": "lib/webparts/demandeurDashboard/loc/{locale}.js",
    "ApprobateurDashboardWebPartStrings": "lib/webparts/approbateurDashboard/loc/{locale}.js"
  }
}
